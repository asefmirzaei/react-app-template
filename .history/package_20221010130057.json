{
  "name": "appp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:staged": "lint-staged",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier \"src/**/*.{ts,tsx}\"",
    "start:dev": "NODE_ENV=development webpack serve",
    "test": "npm run test:jest && npm run test:cypress",
    "test:jest": "jest",
    "test:watch": "jest --verbose --watchAll --no-cache --runInBand",
    "test:cypress": "cypress run",
    "tsc:check": "npm run tsc:src && npm run tsc:cypress",
    "tsc:src": "tsc --noEmit --project tsconfig.json",
    "tsc:cypress": "tsc --noEmit --project cypress/tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.19.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-loader": "^8.2.5",
    "cypress": "^10.9.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "optionalDependencies": {
    "win-node-env": "^0.6.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run prettier:fix",
      "npm run lint",
      "npm run tsc:check",
      "npm run tsc:cypress",
      "git add"
    ]
  },
  "huskey":{
    "hooks":{
      "pre-commit":"lint-staged && npm run test && npm run test:cypress"
    }
  }
}
